{
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": ["error"],
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": ["registration"]
      }
    ]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": ["node", "webpack"]
  }
}
