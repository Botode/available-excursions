{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": ["error"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": ["node", "webpack"]
  }
}
