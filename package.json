{
  "name": "course",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -e development webpack --mode development",
    "build": "env-cmd -e production webpack --mode production",
    "watch": "env-cmd -e development webpack --mode development --watch",
    "start": "env-cmd -e development webpack-dev-server --mode development --open",
    "stats": "env-cmd -e development --use-shell \"webpack --json > stats.json && webpack-bundle-analyzer stats.json\"",
    "lint": "env-cmd -e development eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "env-cmd -e development eslint . --fix --ext .js,.ts,.jsx,.tsx",
    "server:start": "env-cmd -f .env.development.local --use-shell nodemon --exec yarn run babel-node src/server/server.js",
    "server:test": "env-cmd -e test jest",
    "server:test1": "env-cmd -e test babel-node src/server/test.js",
    "ts:build": "tsc -p tsconfig.json",
    "ts:start": "yarn node index.js",
    "migrate": "env-cmd -f .env.development.local sequelize migration:generate --name Excursion",
    "testin": "env-cmd -f .env.development.local yarn node testing.js",
    "generate": "env-cmd -f .env.development.local sequelize model:create --name User --attributes name:string,state:boolean,birth:date,card:integer,role:enum:'{Admin,Guest}'",
    "seed": "env-cmd -f .env.development.local sequelize seed:generate --name demo-user",
    "migrate:auto": "env-cmd -f .env.development.local makemigration --name Excursion",
    "info": "env-cmd -f .env.development.local sequelize db:migrate"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "installConfig": {
    "pnp": true
  },
  "dependencies": {
    "@yarnpkg/pnpify": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "env-cmd": "^10.1.0",
    "fastify": "^3.0.2",
    "fastify-autoload": "^3.0.3",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "normalize": "^0.3.1",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^6.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "3",
    "css-loader": "^3.6.0",
    "csv-loader": "^3.0.3",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "jest-config": "^26.1.0",
    "json-schema-to-typescript": "^9.1.1",
    "less": "^3.12.0",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.13.5",
    "sass-loader": "^9.0.2",
    "sequelize-cli": "^6.2.0",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "xml-loader": "^1.2.1"
  }
}
